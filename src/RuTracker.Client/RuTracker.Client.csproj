<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <LangVersion>9.0</LangVersion>
    <Nullable>enable</Nullable>
    <NullableContextOptions>enable</NullableContextOptions>
    <WarningsAsErrors>nulalble</WarningsAsErrors>
  </PropertyGroup>

  <PropertyGroup>
    <Version>0.8.0</Version>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>RuTracker.Client</PackageId>
    <PackageProjectUrl>https://github.com/ilyalatt/RuTracker.Client</PackageProjectUrl>
    <PackageIconUrl>https://rutracker.org/favicon.ico</PackageIconUrl>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Description>rutracker.org client</Description>
    <Authors>ilyalatt</Authors>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="AngleSharp" Version="0.16.1" />
    <PackageReference Include="IsExternalInit" Version="1.0.2">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="System.Text.Encoding.CodePages" Version="6.0.0" />
  </ItemGroup>

</Project>
